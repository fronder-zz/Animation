# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

def fabric_pods

pod 'AFNetworking', '~> 3.0'
pod 'RealmSwift'
pod 'SwiftyJSON', '~> 4.0'

end

target 'AccentureTask' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for AccentureTask
  fabric_pods

  target 'AccentureTaskTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AccentureTaskUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
